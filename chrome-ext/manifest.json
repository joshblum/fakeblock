{
    "background": {
        "page": "html/background.html"
    },
    "browser_action": {
        "default_icon": "img/snake2.png",
        "default_popup": "html/popup.html",
        "default_title": "ParselTongue"
    },
    "content_scripts": [
        {
            "js": [
                "libs/jquery-1.10.2.min.js",
                "libs/jquery-1.10.2.min.map",
                "js/content_scripts/common.js",
                "js/content_scripts/gmail_overlay.js",
                "js/content_scripts/main.js",
                "js/content_scripts/grab_token.js",
                "js/content_scripts/registration.js"

            ],
            "matches": [
                "http://127.0.0.1:8000/*",
                "https://mail.google.com/*",
                "http://*.getparseltongue.com/*",
                "http://*.parseltongueextension.com/*",
                "http://127.0.0.1:8000/*"
            ]
        }
    ],
    "web_accessible_resources": [
        "libs/jquery-1.10.2.min.js",
        "libs/jquery-1.10.2.min.map"
    ],
    "description": "ParselTongue",
    "icons": {
        "128": "img/snake2.png",
        "48": "img/snake2.png"
    },
    "homepage_url": "http://www.parseltongueextension.com/",
    "manifest_version": 2,
    "name": "ParselTongue",
    "permissions": [
        "http://www.parseltongueextension.com/*",
        "http://www.getparseltongue.com/*",
        "http://127.0.0.1:8000/*",
        "tabs"
    ],
    "version": "0.0.0"
}