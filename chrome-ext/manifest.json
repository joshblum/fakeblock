{
    "background": {
        "page": "html/background.html"
    }, 
    "browser_action": {
        "default_icon": "img/lock-128.png",
        "default_popup": "html/popup.html", 
        "default_title": "Fakeblock"
    }, 
    "content_scripts": [
        {
            "js": [
                "libs/jquery-1.10.1.min.js",
                "js/popup/popup.js",
                "js/content_scripts/facebook_overlay.js",
                "js/content_scripts/main.js",
                "js/content_scripts/grab_token.js"
            ], 
            "matches": [
                "https://*.facebook.com/*",
                "http://127.0.0.1:8000/done_token/"
            ]
        }
    ], 
    "description": "Fakeblock", 
    "icons": {
        "128": "img/lock-128.png"
        //"48": "img/lock-48.png"
    }, 
    "manifest_version": 2, 
    "name": "Fakeblock", 
    "permissions": [
        "http://fakeblock.herokuapp.com/*", 
        "http://localhost:8000/*", 
        "cookies", 
        "tabs"
    ], 
    "version": "0.0.0"
}