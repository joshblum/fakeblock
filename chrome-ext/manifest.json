{
    "background": {
        "page": "html/background.html"
    }, 
    "browser_action": {
        "default_icon": "img/snake2.png",
        "default_popup": "html/popup.html", 
        "default_title": "Fakeblock"
    }, 
    "content_scripts": [
        {
            "js": [
                "libs/jquery-1.10.1.min.js",
               "js/content_scripts/common.js",
                "js/content_scripts/facebook_overlay.js",
                "js/content_scripts/main.js",
                "js/content_scripts/grab_token.js"
            ], 
            "matches": [
                "https://*.facebook.com/*",
                "http://172.23.148.40/*",
                "http://fakeblock.herokuapp.com/*"
            ]
        }
    ], 
    "description": "Fakeblock", 
    "icons": {
        "128": "img/lock-128.png"
        //"48": "img/lock-48.png"
    },
    "homepage_url": "http://fakeblock.herokuapp.com/*",
    "manifest_version": 2,
    "name": "Fakeblock",
    "permissions": [
        "http://fakeblock.herokuapp.com/*",
        "http://172.23.148.40/*"
    ], 
    "version": "0.0.0"
}